{
  "name": "deployer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "backend": "twilio-run start --functions-folder ./build/functions -p 3001",
    "frontend": "BROWSER='google chrome' react-scripts start",
    "build-css": "npx tailwindcss -i ./src/index.css -o ./assets/style.css --watch",
    "build-assets": "webpack --config webpack.config.js",
    "dev": "concurrently \"npm run frontend\" \"npx tsc --watch\" \"npm run backend\" \"npm run build-css\"",
    "deploy": "twilio-run deploy",
    "test": "npm run build-assets && npm run start"
  },
  "dependencies": {
    "@twilio-labs/serverless-runtime-types": "2.2.2",
    "@twilio/runtime-handler": "1.2.1",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "axios": "0.26.0",
    "concurrently": "7.0.0",
    "react-scripts": "5.0.0",
    "twilio": "^3.56"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "autoprefixer": "10.4.2",
    "babel-loader": "8.2.3",
    "postcss": "8.4.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.49.7",
    "sass-loader": "12.6.0",
    "tailwindcss": "3.0.17",
    "twilio-run": "^3.4.0",
    "typescript": "4.5.5",
    "webpack": "5.67.0",
    "webpack-cli": "4.9.2"
  },
  "engines": {
    "node": "12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
